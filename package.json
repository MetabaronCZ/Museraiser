{
  "name": "museraiser",
  "version": "0.0.1",
  "author": "Milan K.",
  "description": "Music sequencer",
  "main": "dist/index.js",
  "scripts": {
    "clean": "gulp clean",
    "watch": "gulp dev",
    "start": "electron dist/index.js dev",
    "build": "npm i & gulp build & electron-builder"
  },
  "build": {
    "artifactName": "${name}.${ext}",
